<h1>New Project </h1>


	<%= form_with scope: :article, local: true do |form| %>
	  <p>
	    	<%= form.label :title %><br>
	    	<%= form.text_field :title %>
	  </p>
	 
	  <p>
	    	<%= form.label :description %><br>
	    	<%= form.text_area :description %>
	  </p>
	  <p>
			<%= f.label :due_date %><br>
			<%= f.datetime_local_field :due_date %>
		</p>
	 
	  <p>
	    	<%= form.submit %>
	  </p>
	<% end %>

<%= link_to 'Back', root_path %>